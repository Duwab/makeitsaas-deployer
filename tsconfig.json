{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "baseUrl": "./app",
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "paths": {
      "@fake": [
        "../config/fake"
      ],
      "@session": [
        "lib/session"
      ],
      "@databases": [
        "repository/databases"
      ],
      "@models": [
        "lib/models"
      ],
      "@errors": [
        "lib/errors"
      ],
      "@operators": [
        "lib/operators"
      ],
      "@ansible": [
        "../ansible"
      ],
      "@utils": [
        "lib/utils"
      ],
      "@entities": [
        "repository/entities"
      ],
      "@entities/*": [
        "repository/entities/*"
      ]
    }
  }
}
